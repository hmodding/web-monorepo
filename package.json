{
  "name": "@raftmodding/website",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "npm run build --workspaces",
    "start": "concurrently --kill-others \"npm run start --workspace=@raftmodding/client\" \"npm run start --workspace=@raftmodding/server\"",
    "dev": "concurrently --kill-others \"npm run dev --workspace=@raftmodding/client\" \"npm run dev --workspace=@raftmodding/server\"",
    "prettier": "npx prettier --write .",
    "clean": "rimraf \"**/build\" \"**/dist\" \"**/*.tsbuildinfo\"",
    "lint": "",
    "db:setup": "cd ./docker && docker compose up -d"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "workspaces": [
    "server",
    "client"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "concurrently": "^6.1.0",
    "eslint": "^8.5.0",
    "prettier": "^2.2.1",
    "rimraf": "^2.7.1",
    "typescript": "^4.2.4",
    "vue-eslint-parser": "^9.1.0"
  }
}
